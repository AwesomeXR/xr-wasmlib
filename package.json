{
  "name": "xr-wasmlib",
  "version": "1.1.4",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.3.1",
    "xr-wasmlib-emcc-build": "^1.0.0",
    "ts-loader": "^9.4.3",
