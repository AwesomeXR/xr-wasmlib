    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4",
    "prettier": "^3.0.2",
    "release-it": "^16.1.5"
  },
  "scripts": {
    "ci": "prettier --check src && tsc -p tsconfig.json --noEmit",
