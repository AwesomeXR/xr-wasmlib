{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "lib": ["ES2015"]
  },
  "include": ["src/**/*.ts"]
}
